{
    "name": "rector/rector-symfony",
    "license": "MIT",
    "description": "Rector upgrades rules for Symfony Framework",
    "require": {
        "php": ">=7.3",
        "rector/rector": "dev-main",
        "symplify/rule-doc-generator-contracts": "^9.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/security-core": "^5.2",
        "symfony/security-http": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Rector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\Tests\\": "tests"
        },
        "classmap": [
            "stubs"
        ]
    }
}
